{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "linebreak-style": [
        "error",
        "windows"
      ],
      "eol-last": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "error",
        { "vars": "all", "ignoreRestSiblings": false }
      ],
      "no-shadow": ["error", { "builtinGlobals": false, "hoist": "functions", "allow": [], "ignoreOnInitialization": true }]
    }
  }